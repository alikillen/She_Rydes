ting from \"../components/Rating\"\r\n// import Meta from '../components/Meta'\r\nimport {\r\n  listProductDetails,\r\n  createProductReview,\r\n} from '../actions/productActions'\r\nimport { PRODUCT_CREATE_REVIEW_RESET } from '../constants/productConstants'\r\n\r\nconst ProductScreen = ({ history, match }) => {\r\n  // using useState to setup quantity of items on product page\r\n  const [qty, setQty] = useState(1)\r\n  const [rating, setRating] = useState(0)\r\n  const [comment, setComment] = useState(\"\")\r\n\r\n  // const [color, setColor] = useState()\r\n  // const [size, setSize ] = useState()\r\n\r\n  const dispatch = useDispatch()\r\n\r\n  // getting product data from the state using useSelector\r\n  const productDetails = useSelector((state) => state.productDetails)\r\n  const { loading, error, product } = productDetails\r\n\r\n  const userLogin = useSelector((state) => state.userLogin)\r\n  const { userInfo } = userLogin\r\n\r\n  const productReviewCreate = useSelector((state) => state.productReviewCreate)\r\n  const {\r\n    success: successProductReview,\r\n    loading: loadingProductReview,\r\n    error: errorProductReview,\r\n  } = productReviewCreate\r\n  \r\n  useEffect(() => {\r\n    if (successProductReview) {\r\n      setRating(0)\r\n      setComment('')\r\n    }\r\n    if (!product._id || product._id !== match.params.id) {\r\n      dispatch(listProductDetails(match.params.id))\r\n      dispatch({ type: PRODUCT_CREATE_REVIEW_RESET })\r\n    }\r\n  }, [dispatch, match, successProductReview])\r\n\r\n  // this is executed when we add things to our cart using button on product page\r\n  // change addtocarthandler to include size and color\r\n  const addToCartHandler = () => {\r\n    history.push(`/cart/${match.params.id}?qty=${qty}`)\r\n  }\r\n\r\n  \r\n  return (\r\n    <>\r\n      <Link className = \"btn btn-light my-3\" to=\"/\">\r\n        Go back\r\n      </Link>\r\n      {loading \r\n      ? <Loader /> \r\n      : error\r\n      ? <Message variant=\"danger\">{error}</Message>\r\n      : (\r\n        <>\r\n        <Row>\r\n        <Col md={6}>\r\n          <Image src={product.image} alt={product.name} fluid />\r\n        </Col>\r\n        <Col md={3}>\r\n          <ListGroup variant=\"flush\">\r\n            <ListGroup.Item>\r\n              <h3>{product.name}</h3>\r\n            </ListGroup.Item>\r\n            <ListGroup.Item>\r\n              <Rating \r\n              value={product.rating} \r\n              text={`${product.numReviews} reviews`}\r\n\r\n              />\r\n            </ListGroup.Item>\r\n            <ListGroup.Item>\r\n              Price: ${product.price}\r\n            </ListGroup.Item>\r\n            <ListGroup.Item>\r\n              Description: {product.description}\r\n            </ListGroup.Item>\r\n          </ListGroup>\r\n        </Col>\r\n        <Col md={3}>\r\n          <Card>\r\n            <ListGroup variant = \"flush\">\r\n              <ListGroup.Item>\r\n                <Row>\r\n                  <Col>\r\n                    Price:\r\n                  </Col>\r\n                  <Col>\r\n                    <strong>${product.price}</strong>\r\n                  </Col>\r\n                </Row>\r\n              </ListGroup.Item>\r\n\r\n              <ListGroup.Item>\r\n                <Row>\r\n                  <Col>\r\n                    Status:\r\n                  </Col>\r\n                  <Col>\r\n                    {product.countInStock > 0 ? \"In Stock\" : \"Out of Stock\"}\r\n                  </Col>\r\n                </Row>\r\n              </ListGroup.Item>\r\n\r\n                 {/* render size and color dropdowns- (error messages if colour or size not in stock?)\r\n                  make setColour and setSize methods, set dropdowns to correct sizes/colors for jackets, pants and gloves -disable in dropdown if out of stock?? inventory? */}\r\n\r\n                 {product.countInStock > 0 && (\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Size</Col>\r\n                    <Col>\r\n                      <Form.Control \r\n                      as=\"select\" \r\n                      value={qty}\r\n                      onChange={(e) => setQty(e.target.value)}\r\n                      >\r\n                        {[...Array(product.countInStock).keys()].map(\r\n                          (x) => (\r\n                          <option key={x + 1} value={x + 1}>\r\n                            {x + 1}\r\n                          </option>\r\n                        )\r\n                        )}\r\n\r\n                      </Form.Control>\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n              )}\r\n\r\n              {product.countInStock > 0 && (\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Colour</Col>\r\n                    <Col>\r\n                      <Form.Control \r\n                      as=\"select\" \r\n                      value={qty}\r\n                      onChange={(e) => setQty(e.target.value)}\r\n                      >\r\n                        {[...Array(product.countInStock).keys()].map(\r\n                          (x) => (\r\n                          <option key={x + 1} value={x + 1}>\r\n                            {x + 1}\r\n                          </option>\r\n                        )\r\n                        )}\r\n\r\n                      </Form.Control>\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n              )}\r\n\r\n\r\n\r\n              {product.countInStock > 0 && (\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Qty</Col>\r\n                    <Col>\r\n                      <Form.Control \r\n                      as=\"select\" \r\n                      value={qty}\r\n                      onChange={(e) => setQty(e.target.value)}\r\n                      >\r\n                        {[...Array(product.countInStock).keys()].map(\r\n                          (x) => (\r\n                          <option key={x + 1} value={x + 1}>\r\n                            {x + 1}\r\n                          </option>\r\n                        )\r\n                        )}\r\n\r\n                      </Form.Control>\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n              )}\r\n\r\n              <ListGroup.Item>\r\n                <Button \r\n                onClick={addToCartHandler}\r\n                className=\"btn-block\" \r\n                type=\"button\" \r\n                disabled={product.countInStock === 0}>\r\n                  Add to Cart\r\n                </Button>\r\n              </ListGroup.Item>\r\n\r\n           \r\n              \r\n    \r\n\r\n\r\n\r\n            </ListGroup>\r\n          </Card>\r\n        </Col>\r\n      </Row>\r\n\r\n      {/* <Row>\r\n        <Col md={6}>\r\n          <h2>Reviews</h2>\r\n          {product.reviews.length === 0 && <Message>No reviews yet for this product</Message>}\r\n          <ListGroup variant=\"flush\">\r\n            {product.reviews.map(review => (\r\n              <ListGroup.Item key={review._id}>\r\n                <strong>{review.name}</strong>\r\n                <Rating value={review.rating} />\r\n                <p>{review.createdAt.substring(0, 10)}</p>\r\n                <p>{review.comment}</p>\r\n              </ListGroup.Item>\r\n            ))}\r\n              <ListGroup.item>\r\n                <h2>Write a Customer Review</h2>\r\n                {userInfo ? (\r\n                  <h1></h1>\r\n                  ) : (\r\n                    <Message> \r\n                    Please <Link to=\"/login\">login</Link> to write a review. \r\n                      {' '}\r\n                    </Message>\r\n                    )}\r\n              </ListGroup.item>\r\n\r\n          </ListGroup>                \r\n\r\n        </Col>\r\n      </Row> */}\r\n      </>\r\n      )}\r\n     \r\n    </>\r\n  )\r\n}\r\n\r\nexport default ProductScreen\r\n","/mnt/c/Programming/projects/she_rydes/frontend/src/screens/RegisterScreen.js",[],["227","228"],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/UserListScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/AllProductsScreen.js",["229","230"],"import React, { Component, useEffect } from 'react'\r\nimport { useDispatch, useSelector } from \"react-redux\"\r\nimport {Row, Col, Container} from \"react-bootstrap\"\r\nimport Product from \"../components/Product\"\r\nimport Message from \"../components/Message\"\r\nimport Loader from \"../components/Loader\"\r\nimport TopProducts from \"../components/TopProducts\"\r\nimport { listProducts } from \"../actions/productActions.js\"\r\n\r\nconst AllProductsScreen = () => {\r\n  const dispatch = useDispatch()\r\n\r\n  const productList = useSelector(state => state.productList)\r\n  const { loading, error, products } = productList\r\n  // these are parts of the state that could be sent down\r\n\r\n  useEffect(() => {\r\n    dispatch(listProducts())\r\n  }, [dispatch])\r\n  // this calls listProducts and fills up our state by passing the payloads into the reducer\r\n\r\n\r\n\r\n  return (\r\n    <>\r\n\r\n    <TopProducts />\r\n\r\n\r\n      <h1>All Products</h1>\r\n      {loading ? (\r\n        <Loader /> )\r\n        : error ? (\r\n          <Message variant=\"danger\">{error}</Message>\r\n        ) : (\r\n      <Row>\r\n          {products.map(product=>\r\n            <Col key={product._id} sm={12} md={6} lg={4} xl={3}>\r\n              <Product product={product} />\r\n            </Col>\r\n          )}\r\n        </Row>) \r\n      }\r\n        \r\n    </>\r\n  )\r\n}\r\n\r\nexport default AllProductsScreen","/mnt/c/Programming/projects/she_rydes/frontend/src/screens/GlovesScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/ProductEditScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/ShippingandReturnsScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/TermsandConditionsScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/SizingChartsScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/constants/productConstants.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/constants/cartConstants.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/components/Product.js",["231"],"import React, { Component } from 'react'\r\nimport { Link } from \"react-router-dom\"\r\nimport { Card } from 'react-bootstrap'\r\nimport Rating from \"./Rating\"\r\n\r\n// need to change this to also list product color variations? and then options on productscreen to change color\r\n\r\nconst Product = ({product}) => {\r\n  return (\r\n    <Card className=\"my-3 p-3 rounded\">\r\n      <Link to={`/product/${product._id}`}>\r\n        <Card.Img src={product.image} variant=\"top\" />\r\n      </Link>\r\n      \r\n      <Card.Body>\r\n        <Link to={`/product/${product._id}`}>\r\n          <Card.Title as=\"div\">\r\n          <strong>{product.name}</strong>\r\n          </Card.Title>\r\n        </Link>\r\n\r\n        <Card.Text as=\"div\">\r\n          <Rating value={product.rating} text={`${product.numReviews} reviews`}/>\r\n        </Card.Text>\r\n\r\n        <Card.Text as = \"h3\">\r\n          ${product.price}\r\n        </Card.Text>\r\n\r\n      </Card.Body>\r\n    </Card>\r\n  )\r\n}\r\n\r\nexport default Product",["232","233"],"/mnt/c/Programming/projects/she_rydes/frontend/src/components/FormContainer.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/actions/orderActions.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/reducers/cartReducers.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/reducers/orderReducers.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/components/Header.js",["234"],"import React, { Component } from 'react'\r\nimport { useDispatch, useSelector } from \"react-redux\"\r\nimport { LinkContainer } from \"react-router-bootstrap\"\r\nimport {Navbar, Nav, Container,Row, Col, Image, NavDropdown } from \"react-bootstrap\"\r\nimport { logout } from \"../actions/userActions\"\r\n\r\nconst Header = () => {\r\n    const dispatch = useDispatch()\r\n\r\n    const userLogin = useSelector((state) => state.userLogin)\r\n    const {userInfo } = userLogin\r\n\r\n    const logoutHandler = () => {\r\n      dispatch(logout())\r\n    }\r\n\r\n    return (\r\n        <header>\r\n          <div \r\n            style={\r\n              {\r\n                display: \"flex\",\r\n                justifyContent: \"center\",\r\n                alignItems: \"center\"\r\n              }\r\n            }>\r\n            <Row>\r\n              <Col><Image src=\"/images/Logos/logo-desktop.png\" className=\"custom-logo\" /></Col>\r\n            </Row>\r\n          </div> \r\n          \r\n          \r\n          <Navbar variant=\"dark\" expand=\"lg\" collapseOnSelect className='custom-navbar'> \r\n          <Container>\r\n          \r\n            <Navbar.Toggle aria-controls=\"basic-navbar-nav\" />\r\n            <Navbar.Collapse id=\"basic-navbar-nav\">\r\n\r\n              <Nav className=\"custom-nav ml-auto\">\r\n                    <LinkContainer to =\"/\">\r\n                      <Nav.Link>\r\n                        Home\r\n                      </Nav.Link>\r\n                    </LinkContainer>\r\n\r\n                    <LinkContainer to =\"/about\">\r\n                      <Nav.Link>\r\n                        About\r\n                      </Nav.Link>\r\n                    </LinkContainer>\r\n\r\n                    <NavDropdown title=\"Products\" id=\"basic-nav-dropdown\">\r\n\r\n                      <NavDropdown.Item>\r\n                        <LinkContainer to =\"/AllProducts\">\r\n                          <Nav.Link>\r\n                            All Products\r\n                          </Nav.Link>\r\n                        </LinkContainer>\r\n                      </NavDropdown.Item>\r\n\r\n                      <NavDropdown.Item>\r\n                        <LinkContainer to =\"/Jacket\">\r\n                          <Nav.Link>\r\n                            Jackets\r\n                          </Nav.Link>\r\n                        </LinkContainer>\r\n                      </NavDropdown.Item>\r\n\r\n                      <NavDropdown.Item>\r\n                        <LinkContainer to =\"/Pants\">\r\n                          <Nav.Link>\r\n                            Pants\r\n                          </Nav.Link>\r\n                        </LinkContainer>\r\n                      </NavDropdown.Item>\r\n\r\n                      <NavDropdown.Item>\r\n                        <LinkContainer to =\"/Gloves\">\r\n                          <Nav.Link>\r\n                            Gloves\r\n                          </Nav.Link>\r\n                        </LinkContainer>\r\n                      </NavDropdown.Item>\r\n\r\n                      <NavDropdown.Item>\r\n                        <LinkContainer to =\"/StickersandPatches\">\r\n                          <Nav.Link>\r\n                            Stickers and Patches\r\n                          </Nav.Link>\r\n                        </LinkContainer>\r\n                      </NavDropdown.Item>\r\n                    \r\n                    </NavDropdown>\r\n\r\n              </Nav>\r\n\r\n\r\n                <Nav className=\"custom-nav ml-auto\">\r\n                    <LinkContainer to =\"/cart\">\r\n                  <Nav.Link><i className=\"fas fa-shopping-cart\"></i>\r\n                    Cart\r\n                    </Nav.Link>\r\n                  </LinkContainer>\r\n\r\n                  {userInfo ? (\r\n                    <NavDropdown title={userInfo.name} id=\"username\">\r\n                      <LinkContainer to=\"/profile\">\r\n                        <NavDropdown.Item>Profile</NavDropdown.Item>\r\n                      </LinkContainer>\r\n                      <NavDropdown.Item onClick={logoutHandler}>\r\n                        Logout\r\n                      </NavDropdown.Item>\r\n                    </NavDropdown>\r\n                  ) : <LinkContainer to =\"/login\">\r\n                    <Nav.Link><i className=\"fas fa-user\"></i>\r\n                    Login\r\n                    </Nav.Link>\r\n                  </LinkContainer> }\r\n\r\n                  {userInfo && userInfo.isAdmin && (\r\n                    <NavDropdown title='admin' id=\"adminmenu\">\r\n                      <LinkContainer to=\"/admin/userlist\">\r\n                        <NavDropdown.Item>Users</NavDropdown.Item>\r\n                      </LinkContainer>\r\n                     \r\n                      <LinkContainer to=\"admin/productlist\">\r\n                        <NavDropdown.Item>Products</NavDropdown.Item>\r\n                      </LinkContainer>\r\n\r\n                      <LinkContainer to=\"/admin/orderlist\">\r\n                        <NavDropdown.Item>Orders</NavDropdown.Item>\r\n                      </LinkContainer>\r\n\r\n                    </NavDropdown>\r\n                  )}\r\n                \r\n              </Nav>       \r\n              \r\n            </Navbar.Collapse>\r\n            </Container>\r\n          </Navbar>\r\n        </header>\r\n      \r\n    )\r\n}\r\n\r\nexport default Header\r\n","/mnt/c/Programming/projects/she_rydes/frontend/src/screens/CartScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/ShippingScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/PlaceOrderScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/404.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/UserEditScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/JacketsScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/screens/ContactScreen.js",["235","236"],"import React, { useState, useEffect } from 'react'\r\nimport { Link } from 'react-router-dom'\r\nimport { Form, Button, Row, Col } from 'react-bootstrap'\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport Message from '../components/Message'\r\nimport Loader from '../components/Loader'\r\nimport FormContainer from '../components/FormContainer'\r\nimport { register } from '../actions/userActions'\r\n\r\nconst ContactScreen = ({ location, history }) => {\r\n  const [name, setName] = useState('')\r\n  const [email, setEmail] = useState('')\r\n  const [password, setPassword] = useState('')\r\n  const [confirmPassword, setConfirmPassword] = useState('')\r\n  const [message, setMessage] = useState(null)\r\n\r\n  const dispatch = useDispatch()\r\n\r\n  const userRegister = useSelector((state) => state.userRegister)\r\n  const { loading, error, userInfo } = userRegister\r\n\r\n  const redirect = location.search ? location.search.split('=')[1] : '/'\r\n\r\n  useEffect(() => {\r\n    if (userInfo) {\r\n      history.push(redirect)\r\n    }\r\n  }, [history, userInfo, redirect])\r\n\r\n  const submitHandler = (e) => {\r\n    e.preventDefault()\r\n    if (password !== confirmPassword) {\r\n      setMessage('Passwords do not match')\r\n    } else {\r\n      dispatch(register(name, email, password))\r\n    }\r\n  }\r\n\r\n  return (\r\n    <FormContainer>\r\n      <h1>Contact Page - needs fixing</h1>\r\n      {message && <Message variant='danger'>{message}</Message>}\r\n      {error && <Message variant='danger'>{error}</Message>}\r\n      {loading && <Loader />}\r\n      <Form onSubmit={submitHandler}>\r\n        <Form.Group controlId='name'>\r\n          <Form.Label>Name</Form.Label>\r\n          <Form.Control\r\n            type='name'\r\n            placeholder='Enter name'\r\n            value={name}\r\n            onChange={(e) => setName(e.target.value)}\r\n          ></Form.Control>\r\n        </Form.Group>\r\n\r\n        <Form.Group controlId='email'>\r\n          <Form.Label>Email Address</Form.Label>\r\n          <Form.Control\r\n            type='email'\r\n            placeholder='Enter email'\r\n            value={email}\r\n            onChange={(e) => setEmail(e.target.value)}\r\n          ></Form.Control>\r\n        </Form.Group>\r\n\r\n\r\n        <Button type='submit' variant='primary'>\r\n          Register\r\n        </Button>\r\n      </Form>\r\n\r\n      <Row className='py-3'>\r\n        <Col>\r\n          Already have an Account?{' '}\r\n          <Link to={redirect ? `/login?redirect=${redirect}` : '/login'}>\r\n            Login\r\n          </Link>\r\n        </Col>\r\n      </Row>\r\n    </FormContainer>\r\n  )\r\n}\r\n\r\nexport default ContactScreen","/mnt/c/Programming/projects/she_rydes/frontend/src/screens/OrderListScreen.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/components/Message.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/components/Loader.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/components/Rating.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/components/TopProducts.js",[],"/mnt/c/Programming/projects/she_rydes/frontend/src/App.js",["237"],"import React from \"react\"\n// import {BrowserRouter as Router, Route} from \"react-router-dom\"\nimport {\n  BrowserRouter as Router,\n  Switch,\n  Route,\n  Link,\n  Redirect\n} from \"react-router-dom\";\nimport Header from \"./components/Header\"\nimport Footer from \"./components/Footer\"\nimport HomeScreen from \"./screens/HomeScreen\"\nimport AboutScreen from \"./screens/AboutScreen\"\nimport ProductScreen from \"./screens/ProductScreen\"\nimport CartScreen from \"./screens/CartScreen\"\nimport LoginScreen from \"./screens/LoginScreen\"\nimport RegisterScreen from \"./screens/RegisterScreen\"\nimport ProfileScreen from \"./screens/ProfileScreen\"\nimport ShippingScreen from './screens/ShippingScr